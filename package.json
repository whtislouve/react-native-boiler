{
  "name": "boiler",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "clean": "(cd andoroid && ./gradlew clean)",
    "release": "(cd android && ./gradlew app:assembleRelease) && yarn renameAndroid",
    "all": "yarn install && (cd ios && pod install)",
    "podinstall": "(cd ios && pod install)",
    "reinstall": "rm -rf ./node_modules && yarn install  && (cd ios rm -rf ./Pods && pod install)",
    "tsw": "yarn run tsc --watch",
    "renameAndroid": "chmod u=rwx ./renameReleaseAndroidApk.bash && ./renameReleaseAndroidApk.bash",
    "fixWebView": "chmod 777 removeWebView.sh && sudo ./removeWebView.sh",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.6",
    "@react-navigation/native": "^5.9.2",
    "@react-navigation/stack": "^5.14.1",
    "axios": "^0.19.2",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "react": "16.11.0",
    "react-native": "^0.62.2",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-localization": "^2.1.6",
    "react-native-orientation": "^3.1.3",
    "react-native-reanimated": "^1.9.0",
    "react-native-safe-area-context": "^3.0.7",
    "react-native-screens": "^2.9.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.1",
    "typescript-fsa-redux-thunk": "^2.1.9"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-decorators": "^7.10.3",
    "@babel/runtime": "^7.10.3",
    "@types/jest": "^24.0.24",
    "@types/react-native": "^0.62.0",
    "@types/react-native-orientation": "^5.0.1",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "16.9.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-test-renderer": "16.11.0",
    "redux-logger": "^3.0.6",
    "typescript": "^4.1.3",
    "@types/redux-logger": "^3.0.8"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
